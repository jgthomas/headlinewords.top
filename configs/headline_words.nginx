
server {
        listen 80;
        listen [::]:80;
        server_name headlinewords.top www.headlinewords.top;
        return 301 https://$server_name$request_uri;
}

server {
    listen 443 ssl;
    server_name headlinewords.top www.headlinewords.top;

    include snippets/ssl-params.conf;
    include snippets/ssl-headline_words.conf;

    location / {
        include uwsgi_params;
        uwsgi_pass unix:///var/www/html/headline_words/headline_words.sock;
    }
}
